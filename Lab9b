/*Julio G lab9b*/
#include <stdio.h>
#include <math.h>
#define NROWS 2
#define NCOLS 7
int main()
{
    double wavedata[NROWS][NCOLS]={
        {93.4,100.1,99.8,87.0,88.5,101,97.6},
        {100.2,101.1,97.5,102.4,98.9,100.0,101.5}
    };
    int i,j,count=1;
    double perdiff,mean1,mean2,sum;
    for(i=0;i<NROWS;i++){
        printf("Data set #%d :",count);
        for(j=0;j<NCOLS;j++){
        printf(" %4.1lf ",wavedata[i][j]);
        }
        printf("\n");
        count++;
    }
    
    for(j=0;j<2;j++)
    {
        sum+=wavedata[j][i];
    }
    printf("Mean #1 : %.1lf \n",sum);
    
    sum=0;
    for(i=0;i<2;i++)
    {
        sum+=wavedata[i][j];
    }
    printf("Mean #2 : %.1lf \n",sum);
    
    perdiff=(mean1-mean2)/(0.5*(mean1+mean2))*100;
    printf("Percent Difference = %.2lf",perdiff);
    /*
    count=1;
    for(i=0;i<NROWS;i++){
        printf("Means #%d :",count);
        for(j=0;j<NCOLS;j++){
        printf(" %4.1lf ",wavedata[i][j]);
        }
        printf("\n");
        count++;
    }
    */
    return 0;
}
