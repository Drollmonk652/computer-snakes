/*Julio G lab9b*/
#include <stdio.h>
#include <math.h>
#define NROWS 2
#define NCOLS 7
int main()
{
    double wavedata[NROWS][NCOLS]={
        {93.4,100.1,99.8,87.0,88.5,101,97.6},
        {100.2,101.1,97.5,102.4,98.9,100.0,101.5}
    };
    int i,j,count=1;
    double perdiff,mean1,mean2,sum;
    for(i=0;i<NROWS;i++){
        printf("Data set #%d :",count);
        for(j=0;j<NCOLS;j++){
        printf(" %4.1lf ",wavedata[i][j]);
        }
        printf("\n");
        count++;
    }
    
    for(i=0;i<1;i++){
        for(j=0;j<NCOLS;j++){
        sum+=wavedata[i][j];
        }
        mean1=sum/7;
        printf("Mean #1 : %.2lf",mean1);
        count++;
    }
    sum=0;
    printf("\n");

    for(j=0;j<1;j++){
        for(i=0;i<NCOLS;i++){
        sum+=wavedata[i][j];
        }
        mean1=sum/2;
        printf("Mean #2 : %.2lf",mean1);
        count++;
    }

    perdiff=(mean1-mean2)/(0.5*(mean1+mean2))*100;
    printf("\nPercent Difference = %.2lf%",perdiff);
    return 0;
}
