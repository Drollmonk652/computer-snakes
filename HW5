#include <stdio.h>
#include <math.h>

int main()
{
  double a,b,c,d,x,qf,bc,x1,x2,xi,x1i,x2i,xr,r1;
    
   if(a==0)
   {
       printf("It is not a quadratic\n");
       printf("x = %4.3f",x);
   }
   else
   {
       if((a==0)&&(b==0))
       {
           printf("It is not an equaiton");
       }
       else
       {
           if(d==0)
           {
               printf("The discriminant of the polynomial is %.3f\n",d);
               printf("x = %4.3f",x);
           }
           else
           {
               if(d>0)
               {
                    printf("The discriminant of the polynomial is %.3f\n",d);
               }
               else
               {
                   if(d<0)
                   {
                      printf("The discriminant of the polynomial is %.3f\n",d); 
                   }
               }
           }
       }
   }
    
  return 0;
}

