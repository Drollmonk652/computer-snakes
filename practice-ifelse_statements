#include <stdio.h>
#include <math.h>
#include <string.h>

int main(void)
{

char name[30][30],answer;

printf("Who goes to heaven and who goes to hell?\n\n");
printf("What is your Grim Reaper name?: ");scanf("%s",&name);
printf("so your name is %s?\n",name);
scanf("%s",&answer);
if((answer=='Y')||(answer=='y'))
{
    printf("Great, let's get started!");
}
else
{
    printf("If you cannot decide then you don't deserve to be a Grim Reaper.\n");
    printf("GAME OVER");
}




return 0;

}
