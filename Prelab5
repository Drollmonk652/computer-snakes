/*Question 6*/
float a,b,c,bc,d,x1,x2;
printf("Enter A:");scanf("%f",&a);
printf("Enter B:");scanf("%f",&b);
printf("Enter C:");scanf("%f",&c);
d=b*b-4*a*c;
bc=2*a;
x1=(-b-sqrt(d))/bc;
x2=(-b+sqrt(d))/bc;

if(d<0)
{
    printf("The expression is: -%.1f(+/-)sqrt(%.1f*%.1f-4*%.1f*%.1f)n",b,b,b,a,c);
}
else
{
    printf("Error in this term");
}

    return 0;
}
